package aplication;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
		int l = sc.nextInt();
		int c = sc.nextInt();
		int[][] matrix = new int[l][c];
		
		for(int i =0; i<l; i++) {
			for(int j = 0; j<c; j++) {
				matrix[i][j] = sc.nextInt();
			}
		}
		
		
		
		
		
		sc.close();
	}

}
