package aplication;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		
		List<String> lista = new ArrayList<>();
	
		lista.add("joão");
		lista.add("Maria");
		lista.add("José");
		lista.add("Jesus");
		lista.add("Pedro");
		
		int c =0;
		for(String mostrar: lista) {
			c++;
			System.out.println("Nomes que estão na "+c+"° minha lista: " + mostrar);
			
		}
	
	}

}
